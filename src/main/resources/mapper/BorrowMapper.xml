<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.celivra.bookms.Mapper.BorrowMapper">
    <insert id="insertBorrow" parameterType="com.celivra.bookms.Entity.Borrow">
        insert into borrow
        values(
            #{userid},
            #{bookid},
            #{borrowDate},
            #{returnDate}
        )
    </insert>
</mapper>

